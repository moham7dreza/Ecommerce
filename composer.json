{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "ext-curl": "*",
        "ext-json": "*",
        "artesaos/seotools": "^1.0",
        "cviebrock/eloquent-sluggable": "^9.0",
        "cyrildewit/eloquent-viewable": "^6.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "intervention/imagecache": "^2.5",
        "khill/lavacharts": "3.1.*",
        "laravel/framework": "^9.0",
        "laravel/jetstream": "^2.3",
        "laravel/sanctum": "^2.6",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.5",
        "maatwebsite/excel": "^3.1",
        "morilog/jalali": "^3.2",
        "nwidart/laravel-modules": "^9.0",
        "opcodesio/log-viewer": "^1.7",
        "overtrue/laravel-favorite": "^5.0",
        "overtrue/laravel-follow": "^5.0",
        "overtrue/laravel-like": "^5.0",
        "psr/simple-cache": "2.0",
        "rappasoft/laravel-livewire-tables": "^2.8",
        "realrashid/sweet-alert": "^5.1",
        "saeedpooyanfar/zarinpal": "^5.0",
        "spatie/laravel-activitylog": "^4.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php",
            "app/Helpers/TelegramBot.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Share\\": "Modules/Share/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
